      <inertia ixx="0.00010" ixy="0" ixz="0" iyy="0.00010" iyz="0" izz="0.00010"/>
    </inertial>
  </link>

  <!-- Calf: IMPORTANT: allow 0 so qpos0 doesn't violate limits -->
  <joint name="lf_calf_joint" type="revolute">
    <parent link="lf_thigh"/>
    <child link="lf_calf"/>
    <origin xyz="0 0.00475 -0.05" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit lower="-2.5" upper="0.5" effort="12.0" velocity="10.0"/>
